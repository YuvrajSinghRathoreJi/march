1) 
public class Main {
    public static void main(String[] args) {
short a=22;
byte b=127;
int c=3344;
long d=223333;
double e=22.33;
float f=33.4f;
char g='u';
boolean uv= true;

 System.out.println(a);
        System.out.println(b);
        System.out.println(c);
        System.out.println(d);
        System.out.println(e);
        System.out.println(f);
        System.out.println(g);
        System.out.println(uv);

    }
}
