12)
public class Main {
    public static void main(String[] args) {
      double a=4.5;
        double b=7;
        double c=2.3;
        double d=2;
        double e=34.6;



        System.out.println(a*b+(c-d)+e);
    }
}

————————————————————————
13)
public class Main {
    public static void main(String[] args) {
      int a=22;
      int b=33;




        System.out.println("the real value of a and b is :"+a+"  "+b);
        a=a+b;
        b=a-b;
        a=a-b;
        System.out.println("the swap value of a and b is :"+a+"  "+b );
    }
}

___________________________________________
14)
public class Main {
    public static void main(String[] args) {
      int a=22;
      int b=33;
      int c=0;




        System.out.println("the real value of a and b is :"+a+"  "+b);
        c=a;
        a=b;
        b=c;
        System.out.println("the swap value of a and b is :"+a+"  "+b );
    }
}


