import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println(" enter a number :");
        int a= sc.nextInt();
        if  (a >=10)
        {
            System.out.println("the number is max");
        }
       else
        {
            System.out.println("not match");
        }

    }
}

