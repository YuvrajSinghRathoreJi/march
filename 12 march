Q.24
#include <stdio.h>

int main()
{
    for(int i=1; i<=4; i++){
        for(int j=1; j<=4; j++){
            if(i==4 || j==4 || j==1 || i==1){
                printf("*");
            }
            else if(i==2 && j==2){
                printf("%d",2);
            }
             else if(i==2 && j==3){
                printf("%d",2);
            }
            else if(i==3 && j==3){
                printf("%d",3);
            }
            else if(i==3 && j==2){
                printf("%d",3);
            }
            
            else{
                printf(" ");
            }
        }
            printf("\n");
        
    }
    
    return 0;
}
------------------------------------------------------------------
Q.25
#include <stdio.h>

int main()
{
    for(int i=1; i<=5; i++){
        for(int j=1; j<=5; j++){
            if(i==5 || j==5 || j==1 || i==1){
                printf("*");
            }
            else if(i==2 && j==2){
                printf("%d",2);
            }
             else if(i==2 && j==3){
                printf("%d",3);
            }
            else if(i==2 && j==4){
                printf("%d",4);
            }
            else if(i==3 && j==2){
                printf("%d",2);
            }
             else if(i==3 && j==3){
                printf("%d",3);
            }
            else if(i==3 && j==4){
                printf("%d",4);
            }
            else if(i==4 && j==2){
                printf("%d",2);
            }
             else if(i==4 && j==3){
                printf("%d",3);
            }
            else if(i==4 && j==4){
                printf("%d",4);
            }
            
            else{
                printf(" ");
            }
        }
            printf("\n");
        
    }
    
    return 0;
}
