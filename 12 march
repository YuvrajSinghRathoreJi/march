Q.23
#include <stdio.h>

int main()
{
    for(int i=1; i<=4; i++){
        for(int j=1; j<=4; j++){
            if(i==4 || j==4 || j==1 || i==1){
                printf("*");
            }
            else if(i==2 && j==2){
                printf("*");
            }
            else if(i==3 && j==3){
                printf("*");
            }
            else if(i==4 && j==4){
                printf("*");
            }
            else{
                printf(" ");
            }
        }
            printf("\n");
        
    }
    
    return 0;
}
