Q.26
#include <stdio.h>

int main()
{
    for(int i=5; i>=1; i--){
        for(int j=1; j<=5; j++){
            printf("%d",i);
        }
            printf("\n");
        
    }
    
    return 0;
}
---------------------------------------------------------
Q.27 
#include <stdio.h>

int main()
{
    for(int i=1; i<=5; i++){
        for(int j=5; j>=1; j--){
            printf("%d",j);
        }
            printf("\n");
        
    }
    
    return 0;
}
