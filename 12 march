Q.28 
#include <stdio.h>

int main()
{  int num = 1;
    for(int i=1; i<=5; i++){
        for(int j=1; j<=5; j++){
            
            printf("%d ",num);
            num+=2;
        }
            printf("\n");
        
    }
    
    return 0;
}
---------------------------------------------------------
Q.29
#include <stdio.h>

int main()
{  int num = 1;
    for(int i=1; i<=5; i++){
        for(int j=1; j<=5; j++){
            
            printf("%d ",num);
            num+=5;
        }
            num= i+1;
            printf("\n");
    }
    
    return 0;
}
------------------------------------------------------------
Q.30
#include <stdio.h>

int main()
{  int num = 1;
    for(int i=1; i<=5; i++){
        for(int j=1; j<=5; j++){
            
            printf("%d ",num);
            num+=1;
        }
            num= i+1;
            printf("\n");
    }
    
    return 0;
}
