import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter the value of short ");
        short a = sc.nextShort();
        System.out.println("the value of short is :" + a);
        System.out.println("enter the value of byte");
        byte b = sc.nextByte();
        System.out.println(" the value of byte is : " +b);
        System.out.println(" enter the value of int ");
        int c = sc.nextInt();
        System.out.println( " the  value of int is :" +c);
        System.out.println("enter the value of long :");
        long d = sc.nextLong();
        System.out.println( "the value of long  is :"+d);
        System.out.println( "enter the value of double :");
        double e = sc.nextDouble();
        System.out.println(" the value of double is " +e);
        System.out.println("enter the value of float");
        float f = sc.nextFloat();
        System.out.println("the value of float is  "+f);
        System.out.println(" enter the value of char :");
        char g = sc.next().charAt(0);
        System.out.println( "the value of char is "+g);
        System.out.println("enter the value of boolean :");
        boolean h = sc.nextBoolean();
        System.out.println("the value of boolean is "+h);



    }
}
