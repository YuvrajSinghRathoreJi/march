20) 

import java.util.*;


public class  Main{
    public static void main(String[] args){
        Scanner dc = new Scanner(System.in);

        System.out.println("enter value of n");
        int n = dc.nextInt();

        System.out.println("Enter value of m :");
        int m = dc.nextInt();

        int i = 100;

        while(i>=1){
            if(i%n == 0 && i%m==0 ){
                System.out.println(i);
            }
            i--;
        }
    }
}
