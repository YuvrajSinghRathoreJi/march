21)
public class  Main{
    public static void main(String[] args){
        int a = 2;
        int i = 1;

        while(i<=10){
            System.out.println(a+ "*" +i+ "=" + a*i);
            i++;
        }

        

    }
}

B)
import java.util.Scanner;
public class Yuvraj
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number: ");

        int num=sc.nextInt();
        int i=1;
        while(i <= 10)
        {
            System.out.println(num+" * "+i+" = "+num*i);
            i++;
        }
    }
}
22)
public class Yuvraj
{
    public static void main(String args[]) {
        int i = 1;
        int sum = 0;
        while (i <= 100) {
            if (i % 2 == 0) {
                System.out.println(i);
            }
            i++;
            sum = sum + i;
        }
        System.out.println(sum);

    }
}
23)public class Yuvraj
{
    public static void main(String args[]) {
        int i = 1;
        int sum = 0;
        while (i <= 100) {
            if (i % 2 == 1) {
                System.out.println(i);
            }
            i++;
            sum = sum + i;
        }
        System.out.println(sum);

    }
}

