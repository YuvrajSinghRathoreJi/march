public class Main {

    public static void main(String[] args) {
        int i = 20;

        
        while(i<=30){
            if(i%2==0){
                System.out.println(i);
            }
            i++;
        }
       
    }
}

10)
public class Main {

    public static void main(String[] args) {

        int sum = 0;
        
        for(int i =1 ; i <= 100; i++){
            System.out.println(i);
            sum = sum +i;
        }

        System.out.println("Total is :"+ sum);

        
        
       
    }
}
11)
public class Main {

    public static void main(String[] args) {
        int n1 = 0;
        int n2=1;
        int n3;

        
        for(int i =1 ; i <= 10; i++){
            n3=n1+n2;
            n1=n2;
            n2=n3;

            System.out.println("Total is :"+ n3);


            
        }

       
    }
}
12)
import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner dc= new Scanner(System.in);

        System.out.println("Enter a number :");
        int a = dc.nextInt();

        int fact =1;
        int i=1;

        while(i<=a){
            fact = fact*i;
            i++;
        }
        System.out.println(fact);

       
    }
}
14)
import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner dc= new Scanner(System.in);

        System.out.println("Enter a number :");
        int num = dc.nextInt();

        int rem,sum=0;
        int temp = num;

        while(num>0){
            rem=num%10;
            sum =sum*10+rem;
            num=num/10;
        }

        if(sum==temp){
            System.out.println("palindrome number");
        }
        else {
            System.out.println("Not a palindrome number");
        }

        
       
    }
}

