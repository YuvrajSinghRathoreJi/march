16)
import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner dc = new Scanner(System.in);

        System.out.println("enter Temp :");
        char temp = dc.next().charAt(0);

        if(temp =='c'){

            System.out.println("enter Fahrenheit Value :");
            int f = dc.nextInt();

            int c = (f - 32) * 5/9;

            System.out.println(c);

            

        }


        

    }
}

