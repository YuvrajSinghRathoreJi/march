     char temp = sc.next().charAt(0);


if ( temp=='c') { 
    System.out.println("enter value :");



int f=sc.nextInt();
int c=(f-32)*5/9; 

System.out.println("calcus  "+ c); }

  else if (temp=='f') {
    System.out.println("enter value");
    int c=sc.nextInt();
