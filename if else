import java.util.*;

public class Yuv{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter any one  number 1 to 3  :");
        int manu = sc.nextInt();

        if( manu == 1 ){
            
            System.out.println("Pizza" + "- 399rs/pc ");
            System.out.println("Burger" + "- 79rs/pc ");
            System.out.println("tanduri momo"+"-100rs/6pc");
            System.out.println("strem momo"+"-70rs/6pc");
        }
        else if (manu == 2){
            System.out.println("mater paneer" + "- 179rs/plate ");
            System.out.println("Sahi Paneer" + "- 209rs/plate ");

        }
        else if (manu == 3 ){
            System.out.println("kaju katli" + "- 10rs/pc ");
            System.out.println("gulab jamun" + "- 8rs/pc ");
        }
        else {
            System.out.println("not a manu number");
        }
    }
}

