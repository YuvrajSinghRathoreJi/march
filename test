Q5
#include <stdio.h>

int main() {
    int a,b;
    
    
    printf("Enter starting point: ");
    scanf("%d", &a);
    printf("Enter ending point: ");
    scanf("%d", &b);
    
    printf("Even numbers between %d and %d are:\n",a,b);
    for (int i = a; i <= b; i++) {
        if (i % 2 == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
    
    printf("Odd numbers between %d and %d are:\n", a,b);
    for (int i = a; i <= b; i++) {
        if (i % 2 != 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
    
    return 0;
}
