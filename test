Q2
#include <stdio.h>
int main()
{
  int num;
   printf("Enter Any Number");
   scanf("%d",&num);
   int rem,sum=0;
   int temp=num;
   while(num>0){
       rem=num%10;
       sum=sum+rem*rem*rem;
       num=num/10;
   }
   printf("%d ",sum);
   if(temp==sum){
       printf("this number is armstrong number \n");
   }
   else {
       printf("this number is not armstrong number \n");
   }
    return 0;
} 
