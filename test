Q3
#include <stdio.h>

int main() {
   int num;
    printf("Enter the Number");
    scanf("%d",&num);
    int rem,sum=0;
    while(num>0){
        rem = num%10+1;
        sum = sum*10+rem;
        num = num/10;
    }
    printf("%d",sum);


    return 0;
} 
