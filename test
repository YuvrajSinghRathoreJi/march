Q4
#include <stdio.h>
int main()
{
   int num;
   printf("Enter Any Number");
   scanf("%d",&num);
   int rem,sum=0;
   int temp=num;
   while(num>0){
       rem=num%10;
       sum=sum*10+rem;
       num=num/10;
   }
   printf("%d",sum);
    return 0;
}






