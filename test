Q1

#include <stdio.h>

int main()
{
    int num;
    printf("Enter Any Number");
    scanf("%d",&num);
    printf("%d\n",num*num);
    int sum = num*num;
    int count = 0;
    while(sum>0){
        count += sum % 10;
        sum /= 10;
    }
    printf("%d\n",count);
    if(count==num){
        printf("This is a neon number");
    }
    else{
        printf("This is not a neon number");
    }
    return 0;
}





